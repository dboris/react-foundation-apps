    "accordion",
    "tabs",
    "offcanvas",
    "interchange",
    "notification",
    "actionsheet",
    "react-component"
  ],
  "author": {
    "name": "Kiran Abburi"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akiran/react-foundation-apps/issues"
  },
  "homepage": "https://github.com/akiran/react-foundation-apps",
  "devDependencies": {
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.0.15",
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.22.0",
    "del": "^2.0.2",
    "escape-loader": "*",
    "gulp": "^3.8.10",
    "gulp-sass": "^2.0.1",
    "highlight.js": "^8.4.0",
    "highlightjs-themes": "git+https://github.com/akiran/highlightjs-themes",
    "html-loader": "^0.3.0",
    "jsx-loader": "^0.13.2",
    "markdown-loader": "^0.1.2",
    "multiline": "^1.0.1",
    "postcss-loader": "^0.7.0",
    "raw-loader": "^0.5.1",
    "react": "*",
    "react-highlight": "git+https://github.com/akiran/react-highlight.git",
    "react-hot-loader": "^1.2.3",
    "react-layer-mixin": "*",
    "react-responsive-mixin": "^0.3.*",
    "react-router": ">=1.0.0-rc4",
    "run-sequence": "^1.0.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-responsive-mixin": "*",
    "pubsub-js": "^1.5.x"
  },
  "dependencies": {
    "can-use-dom": "^0.1.0",
    "object-assign": "*",
    "pubsub-js": "^1.5.0",
    "tether": "^1.1.1"
  },
  "babel": {
    "presets": ["es2015", "react"]
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "npmName": "react-foundation-apps",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ]
